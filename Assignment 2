#include <iostream>

using namespace std;

int main()
{
   int num1,num2;
    cout<<"enter the  first number"<<endl;
    cin>>num1;
    cout<<"enter the 2nd number"<<endl;
    cin>>num2;
    if(num1>num2){
        cout<<"num1 is geatest"<<num1<<endl;
    }
    else{
        cout<<"num2 is greatest"<<num2<<endl;
    }
    

    return 0;
